{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "types": [
            "node",
            "@playwright/test"
        ],
        "baseUrl": ".",
        "paths": {
            "@pages/*": [
                "pages/*"
            ],
            "@fixtures/*": [
                "fixtures/*"
            ],
            "@data/*": [
                "data/*"
            ]
        }
    },
    "include": [
        "**/*.ts"
    ]
}